page {
    background-color: #fafafa;
}

.tab-wrap {
    z-index:9999;
    position: fixed;
    top: 0;
    left: 0;
    display: flex;
    justify-content: space-around;
    align-items: center;
    width: 100%;
    height: 90rpx;
    box-shadow: 0 4rpx 7rpx 0 rgba(0, 0, 0, 0.06);
    background-color: #fff;
}

.tab {
    position: relative;
    height: 90rpx;
    line-height: 90rpx;
    font-size: 25rpx;
    color: #303030;
}

.tab-wrap .active {
    color: #3c89ff;
}

.tab-wrap .active::after {
    position: absolute;
    content: '';
    bottom: 0rpx;
    left: 50%;
    width: 100%;
    height: 4rpx;
    transform: translate3d(-50%, 0, 0);
    background-color: #3c89ff;
}

.item {
    width: 716rpx;
    height: 246rpx;
    margin: 0 auto 20rpx;
    background: url('data:image/png;base64,') no-repeat center/100%;
}

.item .top {
    display: flex;
    padding-top: 22rpx;
    padding-left: 25rpx;
    padding-right: 47rpx;
}

.top .info {
    flex: 1;
}

.top .name {
    line-height: 40rpx;
    font-size: 29rpx;
    color: #303030;
}

.top .type {
    height: 29rpx;
    width: max-content;
    margin-top: 13rpx;
    margin-bottom: 25rpx;
    padding: 0 16rpx;
    line-height: 29rpx;
    text-align: center;
    font-size: 18rpx;
    border-radius: 0 14rpx 0 14rpx;
    color: #fff;
    background: linear-gradient(135deg, rgba(111, 190, 255, 1) 0%, rgba(60, 137, 255, 1) 100%);
}

.top .price {
    font-size: 36rpx;
    font-weight: 700;
    color: #fd3c51;
}

.price span {
    line-height: 76rpx;
    font-size: 76rpx;
}

.top .valid {
    line-height: 31rpx;
    font-size: 22rpx;
    color: #999;
}

.item .bot {
    display: flex;
    align-items: center;
    justify-content: flex-end;
    margin-top: 42rpx;
    padding-right: 23rpx;
}

.bot .text {
    line-height: 31rpx;
    font-size: 22rpx;
    color: #3c89ff;
}

.bot .more {
    width: 31rpx;
    height: 31rpx;
}

.coupon-item {
  width: 750rpx;
  height: 215rpx;
  background: url('data:image/png;base64,') no-repeat center/100%;
  display: flex;
  align-items: center;
  color: #fff;
  font-size: 20rpx;
  text-align: center;
  margin: 0 auto 20rpx;
}

.left-box {
  margin-left:70rpx;
  margin-right: 40rpx;
}

.has-use {
  background: url('data:image/png;base64,') no-repeat center/100%;
}