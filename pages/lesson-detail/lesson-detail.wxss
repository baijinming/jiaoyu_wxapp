@import "/wxParse/wxParse.wxss";

.banner {
    width: 694rpx;
    height: 408rpx;
    margin: 0 auto;
    border-radius: 11rpx;
    background-color: #fbfbfb;
}

.head {
    padding: 16rpx 29rpx 24rpx;
    border-bottom: 18rpx solid #fafafa;
}

.head .blank {
    display: flex;
    align-items: center;
    margin: 11rpx 0 18rpx;
}

.blank .badge {
    width: 40rpx;
    height: 40rpx;
}

.blank .name {
    line-height: 40rpx;
    font-size: 28rpx;
    color: #303030;
}

.blank .time {
    flex: 1;
    margin-left: 31rpx;
    line-height: 36rpx;
    font-size: 28rpx;
    color: #999;
}

.blank .like {
    width: 40rpx;
    height: 40rpx;
}

.blank .likenum {
    line-height: 36rpx;
    font-size: 25rpx;
    color: #dbdbdb;
}

.price-wrap {
    width: 694rpx;
    height: 96rpx;
    padding-left: 25rpx;
    border-radius: 7rpx;
    border: 1px solid rgba(243, 236, 224, 1);
    background: rgba(253, 251, 247, 1);
}

.price {
    line-height: 96rpx;
    font-size: 31rpx;
    font-weight: 700;
    color: #ff9607;
}

.cost {
    margin-left: 96rpx;
    line-height: 36rpx;
    font-size: 25rpx;
    color: #303030;
}

.title {
    line-height: 51rpx;
    font-size: 38rpx;
    font-weight: 700;
    color: #303030;
}

.tab-wrap {
    display: flex;
    justify-content: space-around;
    align-items: center;
    width: 100%;
    height: 80rpx;
    border-bottom: 1px solid #f2f2f2;
    background-color: #fff;
}

.tab {
    position: relative;
    height: 80rpx;
    line-height: 80rpx;
    font-size: 25rpx;
    color: #303030;
}

.tab-wrap .active {
    color: #3c89ff;
}

.tab-wrap .active::after {
    position: absolute;
    content: '';
    bottom: 0;
    left: 50%;
    width: 100%;
    height: 4rpx;
    transform: translate3d(-50%, 0, 0);
    background-color: #3c89ff;
}

/* ------------------------------------------- */

.tit {
    padding-left: 29rpx;
    padding-top: 25rpx;
    line-height: 40rpx;
    font-size: 29rpx;
    font-weight: 700;
    color: #303030;
}

.avatar-wrap {
    display: flex;
    align-items: center;
    padding-left: 29rpx;
    margin-top: 34rpx;
}

.avatar {
    width: 94rpx;
    height: 94rpx;
    border-radius: 50%;
    background-color: #fbfbfb;
}

.tname {
    margin-left: 33rpx;
    line-height: 36rpx;
    font-size: 25rpx;
    font-weight: 700;
    color: #303030;
}

.brief {
    padding: 27rpx 10rpx;
    box-sizing: border-box;
    line-height: 36rpx;
    font-size: 25rpx;
    color: #666;
    border-bottom: 1px solid #f2f2f2;
}

.catalog {
    padding: 42rpx 34rpx 0;
}

.ctit {
    margin-bottom: 38rpx;
    line-height: 40rpx;
    font-size: 29rpx;
    font-weight: 700;
    color: #303030;
}

.item {
    display: flex;
    align-items: center;
    margin-bottom: 54rpx;
}

.img {
    width: 200rpx;
    height: 116rpx;
    background-color: #fbfbfb;
}

.info {
    flex: 1;
    margin-left: 18rpx;
    position: relative;
}

.audition {
  width: 80rpx;
  height: 30rpx;
  position: absolute;
  top: 60rpx;
  right: 20rpx;
}

.lname {
    overflow: hidden;
    height: 72rpx;
    line-height: 36rpx;
    font-size: 25rpx;
    color: #3a3939;
}

.long {
    margin-top: 13rpx;
    line-height: 31rpx;
    font-size: 22rpx;
    color: #999;
}

.btn {
    position: fixed;
    left: 0;
    bottom: 0;
    z-index: 66;
    width: 750rpx;
    height: 98rpx;
    font-size: 34rpx;
    line-height: 98rpx;
    text-align: center;
    font-weight: 700;
    color: #fff;
    background: linear-gradient(135deg, rgba(111, 190, 255, 1) 0%, rgba(60, 137, 255, 1) 100%);
}
.swiper-item .img {
    width:100%;
    height:100%;
    border-radius:10rpx;
    background-color:#fbfbfb;
}

.no-detail image{
  width: 400rpx;
  height: 400rpx;
  margin: 150rpx auto 50rpx;
  display: block
}

.no-detail view {
  font-size: 28rpx;
  text-align: center
}

.mask {
  position: fixed;
  left: 0;
  top: 0;
  bottom: 0;
  right: 0;
  background-color: #000;
  opacity: .5;
  z-index: 99;
}

.coupon {
  width: 100%;
  position: fixed;
  left: 0;
  bottom: 0;
  z-index: 100;
  background-color: #fff;
}

.coupon-box-item {
  height: 100rpx;
  padding: 0 30rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
  color: #333;
  font-size: 28rpx;
}

.zf-btn {
  width: 640rpx;
  height: 90rpx;
  margin: 20rpx auto;
  font-size: 34rpx;
  line-height: 90rpx;
  color: #fff;
  text-align: center;
  background: linear-gradient(135deg, rgba(111, 190, 255, 1) 0%, rgba(60, 137, 255, 1) 100%); 
  border-radius: 80rpx;
}

.ash-color {
  color: #999
}

.coupon-list {
  width: 100%;
  height: 0;
  position: fixed;
  left: 0;
  bottom: 0;
  z-index: 9999;
  background-color: #fff;
  transition: all ease 1500ms;
}
.coupon-title {
  font-size: 30rpx;
  color: #b0aaac;
  height: 80rpx;
  line-height: 80rpx;
  text-align: center;
  position: relative;
}
.coupon-close-icon {
  width: 30rpx;
  height: 30rpx;
  position: absolute;
  right: 20rpx;
  top: 25rpx;
}

.coupon-item {
  width: 750rpx;
  height: 215rpx;
  background: url('data:image/png;base64,') no-repeat center/100%;
  display: flex;
  align-items: center;
  color: #fff;
  font-size: 20rpx;
  text-align: center;
  margin: 20rpx auto ;
}

.left-box {
  margin-left:70rpx;
  margin-right: 40rpx;
}
