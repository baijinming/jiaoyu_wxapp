.title {
    margin-bottom: 20rpx;
    padding-top: 128rpx;
    text-align: center;
    line-height: 45rpx;
    font-size: 33rpx;
    font-weight: 700;
    color: #303030;
}

.label {
    margin-left: 136rpx;
    margin-top: 30rpx;
    line-height: 36rpx;
    font-size: 25rpx;
    color: #999;
}

.blank {
    display: flex;
    align-items: center;
    padding: 0 45rpx;
}

.icon {
    width: 65rpx;
    height: 65rpx;
}

input {
    flex: 1;
    height: 65rpx;
    line-height: 65rpx;
    margin-left: 24rpx;
    border-bottom: 1px solid #dbdbdb;
}

.plc {
    font-size: 25rpx;
    color: #303030;
}

.reset {
    margin-top: 18rpx;
    margin-bottom: 80rpx;
    padding-right: 43rpx;
    text-align: right;
    line-height: 30rpx;
    font-size: 22rpx;
    color: #3c89ff;
}

.btn {
    margin: 0 auto 47rpx;
    width: 391rpx;
    height: 91rpx;
    line-height: 91rpx;
    text-align: center;
    font-weight: 700;
    font-size: 31rpx;
    border-radius: 11rpx;
    color: #4794ff;
    border: 2rpx solid rgba(60, 137, 255, 1);
}

.blue {
    border: none;
    color: #fff;
    background: linear-gradient(135deg, rgba(111, 190, 255, 1) 0%, rgba(60, 137, 255, 1) 100%);
}
.in-lo-top {
  height: 400rpx;
  position: relative;
  width: 100%;
}
.in-logo {
  position: absolute;
  top: 120rpx;
  left: 50%;
  width: 180rpx;
  height: 180rpx;
  background: #fff;
  transform: translateX(-50%);
  border-radius: 36rpx;
  box-shadow: 0rpx 0rpx 2rpx 1rpx rgba(204,204,204,0.5);
}
.in-logo-img {
  width: 100%;
  height: 420rpx;
  margin-top: -210rpx;
}
.in-logo-wrap {
  display: flex;
  font-size: 28rpx;
  color: #D4D4D4;
  justify-content: center;
  width: 100%;
  align-items: center;
}
.bottom-logo {
  width: 70rpx;
  height: 70rpx;
}
